/**app.wxss**/
.page {
  height: 100vh;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.card_shadow {
  border: 0 solid #f5f5f5;
  box-shadow: 0 4rpx 20rpx 0 rgba(0, 0, 0, 0.1);
  border-radius: 8rpx;
}
.container {
  display: flex;
  /* 纵向排列 */
  flex-direction: column;
  /* 每项居中 */
  align-items: center;
  /* 两端居中 */
  justify-content: space-around;
  box-sizing: border-box;
  font-family: "微软雅黑";
  overflow: visible;
  color: #333;
}
.page-body {
  display: flex;
  flex-direction: column;
  flex: 1;
  height: 1300rpx;
}
.font-sm {
  font-size: 10pt;
}
